<local:EditorsDemoModule x:Class="EditorsDemo.RangeControlDateSelectorModule"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
             xmlns:dxdb="clr-namespace:DevExpress.Xpf.DemoBase;assembly=DevExpress.Xpf.DemoBase.v15.2"
             xmlns:local="clr-namespace:EditorsDemo"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <dxdb:DemoModuleControl>
        <Grid>
            <dxe:RangeControl x:Name="rangeControl" Grid.Row="1" Height="200" Margin="5,0">
                <dxe:CalendarClient FontSize="16"/>
            </dxe:RangeControl>
        </Grid>
    </dxdb:DemoModuleControl>
</local:EditorsDemoModule>