<local:DiagramDemoModule
    x:Class="DiagramDemo.OfficePlanModule"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxdb="http://schemas.devexpress.com/winfx/2008/xaml/demobase"
        xmlns:local="clr-namespace:DiagramDemo"
        xmlns:dxdiagcore="clr-namespace:DevExpress.Diagram.Core;assembly=DevExpress.Diagram.v15.2.Core"
        xmlns:dxdiag="clr-namespace:DevExpress.Xpf.Diagram;assembly=DevExpress.Xpf.Diagram.v15.2">
    <local:DiagramDemoModule.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/DiagramDemo;component/Resources/OfficePlanResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </local:DiagramDemoModule.Resources>
    <dxdb:DemoModuleControl>
        <Grid>
            <dxdiag:DiagramDesignerControl x:Name="diagramControl" DockPanel.Dock="Right" Theme="Linear" PropertiesPanelVisibility="Collapsed" DefaultStencils="OfficeShapes, BasicShapes"  ZoomFactor="0.6">
            </dxdiag:DiagramDesignerControl>
        </Grid>
    </dxdb:DemoModuleControl>
</local:DiagramDemoModule>